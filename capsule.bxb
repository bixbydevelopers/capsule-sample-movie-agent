capsule {
  id (example.movieAgent)
  version (1.0.0)
  format (3)
  targets {
    target (bixby-mobile-en-US)
    target (bixby-tv-en-US)
    target (bixby-watch-en-US)
    target (bixby-fridge-en-US)
  }
  capsule-imports {
    import (viv.core) {
      as (core)
    }
    import (viv.time) {
      as (time)
      version (3.1.48)
    }
  }
  runtime-version (7) {
    overrides {
      auto-on-click-for-list-of (true)
    }
  }
  store-countries {
    all
  }
  store-sections {
    section (MoviesAndTV)
  }
}
